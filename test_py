
import random
import matplotlib.pyplot as plt

# 랜덤 데이터 생성
x = list(range(1, 21))  # x축: 1~20
y = [random.randint(10, 100) for _ in x]  # y축: 10~100 사이 랜덤 값 생성

# 그래프 그리기
plt.figure(figsize=(10, 5))
plt.plot(x, y, marker='o', linestyle='-', label='Random Data')
plt.title('Random Data Visualization')
plt.xlabel('X-axis')
plt.ylabel('Y-axis')
plt.legend()
plt.grid(True)
plt.show()

